{
  "expo": {
    "name": "SANTA ANA",
    "slug": "SantaAna_Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LOGO.png",
    "scheme": "santaanamobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "processing",
          "processing"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LOGO.png",
        "backgroundColor": "#F9F6EE"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.CAMERA",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "package": "com.anonymous.SantaAna_Mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/LOGO.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F9F6EE"
        }
      ],
      "expo-font",
      [
        "expo-secure-store"
      ],
      [
        "expo-sqlite"
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "kotlinVersion": "2.0.21",
            "javaVersion": "17"
          }
        }
      ],
      "expo-background-task",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9baf2afc-64d5-4bd7-95ff-67560099c705"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9baf2afc-64d5-4bd7-95ff-67560099c705"
    }
  }
}
